{

  "builders": [
   {
    "name": "ubuntu-xenial",
    "type": "amazon-ebs",
    "region": "ca-central-1",
    "source_ami": "ami-e59c2581",
    "instance_type": "t2.micro",
    "ssh_username": "ubuntu",
    "ami_name": "packer_ami {{timestamp | clean_ami_name}}",
    "force_deregister": "True",
    "force_delete_snapshot": "True", 
    "tags": {
      "Os_Version": "ubuntu",
      "Release": "Latest"
      }
   }
  ],

  "provisioners": [
   {
     "type": "shell",
     "execute_command": "{{.Vars}} sudo -E -S bash '{{.Path}}'",
     "scripts": [
	"scripts/initialize.sh"
 	]
   }
   ]

}
